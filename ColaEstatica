public class ColaPI implements ColaTDA{
  int [] a ;
  in t indice;
  public void InicializarCola () {
  a = new in t [100];
  indice = 0;
  }

  public void Acolar(int x){
  a[indice] = x;
  indice++;
  }

  public void Desacolar() {
  for (int i = 0; i < indice -1; i ++)
  a[i] = a[i +1];
  indice - -;
  }

  public boolean ColaVacia (){
  return ( indice == 0) ;
  }

  public int Primero() {
  return a [0];
  }
}
